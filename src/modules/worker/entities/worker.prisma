model Worker {
  id          String    @id @map("_id")
  name        String
  imageId     String?
  imageUrl    String?
  createdUser String
  createdAt   DateTime  @default(now())
  deletedUser String?
  deletedAt   DateTime?
  deleted     Boolean   @default(false)

  userId String

  user User @relation(fields: [userId], references: [id])

  @@map("workers")
}

model User {
  id String @id @map("_id")

  workers Worker[]
}
