model Table {
  id             String    @id @map("_id")
  name           String
  amountOfChairs Float
  disabledAt     DateTime?
  createdUser    String
  createdAt      DateTime  @default(now())
  deletedUser    String?
  deletedAt      DateTime?
  company        Company   @relation(fields: [companyId], references: [id])
  companyId      String
}

model Company {
  id     String  @id @map("_id")
  tables Table[]
}
