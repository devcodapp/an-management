model User {
  id       String @id @map("_id")
  username String
  email    String
  password String

  changePassword Boolean @default(true)

  deletedAt DateTime?

  restaurant   Restaurant @relation(fields: [restaurantId], references: [id])
  restaurantId String
}

model Restaurant {
  id    String @id @map("_id")
  users User[]
}
